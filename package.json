{
  "name": "react-canvas-terminal",
  "version": "0.1.0",
  "description": "WebApp to help you with your overseas expenses",
  "author": "danikaze <danikaze@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/danikaze/react-canvas-terminal"
  },
  "license": "MIT",
  "engines": {
    "node": "10.15.x",
    "npm": "6.4.x"
  },
  "scripts": {
    "dev": "webpack-dev-server --env=dev",
    "build": "npm run clean && npm run build-lib && npm run build-examples",
    "build-lib": "tsc -p tsconfig.json",
    "build-examples": "webpack",
    "clean": "rimraf lib examples",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "test": "npm run lint",
    "prettier": "prettier --write '**/*.{js,ts,jsx,tsx,html}'"
  },
  "devDependencies": {
    "@types/react": "^16.8.17",
    "@types/react-reconciler": "^0.18.0",
    "autoprefixer": "^9.7.3",
